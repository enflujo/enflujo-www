{
  "name": "enfujo-www",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "astro dev --host",
    "build": "astro build",
    "preview": "astro build && astro preview",
    "check": "astro check",
    "lint": "prettier --check .",
    "lint:fix": "prettier --write --list-different .",
    "serve": "yarn server:start",
    "stop": "yarn server:stop",
    "restart": "yarn server:restart",
    "server:start": "yarn build && pm2 start ecosystem.config.js",
    "server:stop": "pm2 stop ecosystem.config.js",
    "server:restart": "pm2 restart ecosystem.config.js"
  },
  "dependencies": {
    "@astrojs/sitemap": "^2.0.2",
    "@enflujo/alquimia": "^2.0.0",
    "@rollup/plugin-graphql": "^2.0.3",
    "astro": "^2.10.12",
    "astro-compress": "^2.0.14",
    "astro-robots-txt": "^0.5.0",
    "autoprefixer": "^10.4.15",
    "marked": "^7.0.4"
  },
  "devDependencies": {
    "@types/marked": "^5.0.1",
    "graphql": "^16.8.0",
    "postcss": "^8.4.28",
    "sass": "^1.66.1"
  }
}
