{
  "name": "enfujo-www",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro build && astro preview",
    "check": "astro check",
    "lint": "prettier --check .",
    "lint:fix": "prettier --write --list-different .",
    "serve": "yarn server:start",
    "stop": "yarn server:stop",
    "restart": "yarn server:restart",
    "server:start": "yarn build && pm2 start ecosystem.config.js",
    "server:stop": "pm2 stop ecosystem.config.js",
    "server:restart": "pm2 restart ecosystem.config.js"
  },
  "dependencies": {
    "@astrojs/sitemap": "^1.2.1",
    "@rollup/plugin-graphql": "^2.0.3",
    "astro": "^2.1.8",
    "astro-robots-txt": "^0.4.1",
    "autoprefixer": "^10.4.14",
    "marked": "^4.3.0"
  },
  "devDependencies": {
    "@types/marked": "^4.0.8",
    "graphql": "^16.6.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "prettier-plugin-astro": "^0.8.0",
    "sass": "^1.60.0"
  }
}
