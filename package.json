{
  "name": "enfujo-www",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "astro dev --host",
    "build": "astro build",
    "preview": "astro build && astro preview",
    "check": "astro check",
    "lint": "prettier --check .",
    "lint:fix": "prettier --write --list-different .",
    "serve": "yarn server:start",
    "stop": "yarn server:stop",
    "restart": "yarn server:restart",
    "server:start": "yarn build && pm2 start ecosystem.config.js",
    "server:stop": "pm2 stop ecosystem.config.js",
    "server:restart": "pm2 restart ecosystem.config.js"
  },
  "dependencies": {
    "@astrojs/sitemap": "^3.0.3",
    "@enflujo/alquimia": "^2.0.0",
    "@rollup/plugin-graphql": "^2.0.4",
    "astro": "^3.5.5",
    "astro-compress": "^2.2.3",
    "astro-robots-txt": "^1.0.0",
    "autoprefixer": "^10.4.16",
    "marked": "^10.0.0"
  },
  "devDependencies": {
    "@types/marked": "^6.0.0",
    "graphql": "^16.8.1",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "sass": "^1.69.5"
  }
}
