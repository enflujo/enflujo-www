{
  "name": "enflujo-www",
  "packageManager": "yarn@4.7.0",
  "version": "1.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "astro dev --host",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "revisar": "astro check",
    "lint": "prettier --check .",
    "lint:fix": "prettier --write --list-different .",
    "serve": "yarn server:start",
    "stop": "yarn server:stop",
    "restart": "yarn server:restart",
    "server:start": "yarn build && pm2 start ecosystem.config.js",
    "server:stop": "pm2 stop ecosystem.config.js",
    "server:restart": "pm2 restart ecosystem.config.js"
  },
  "dependencies": {
    "@astrojs/check": "0.9.4",
    "@astrojs/sitemap": "3.2.1",
    "@enflujo/alquimia": "^2.0.0",
    "@rollup/plugin-graphql": "^2.0.5",
    "@speed-highlight/core": "^1.2.7",
    "astro": "5.4.2",
    "astro-robots-txt": "^1.0.0",
    "astro-seo-schema": "^5.0.0",
    "autoprefixer": "^10.4.20",
    "marked": "^15.0.7",
    "marked-highlight": "^2.2.1",
    "schema-dts": "^1.1.5",
    "typescript": "^5.8.2"
  },
  "devDependencies": {
    "@types/marked": "^6.0.0",
    "graphql": "^16.10.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "sass": "^1.85.1"
  }
}
